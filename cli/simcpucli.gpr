with "config/simcpucli_config.gpr";
with "../bbs_simcpu.gpr";

project simcpucli is
   for languages use ("Ada");
   for Source_Dirs use (".");
   for Object_Dir use "obj";
   for Main use ("simcpucli.adb");
   for Exec_Dir use ".";
   package builder is
      for switches ("Ada") use ("-s");
   end builder;
   package compiler is
      for switches ("Ada") use ("-g", "-gnateE", "-gnata");
   end compiler;
end simcpucli;

